type::{
  name: Person,
  type: struct,
  fields: {
    title: {
      type: symbol,
      valid_values: [
        Mr,
        Mrs,
        Miss,
        Ms,
        Mx,
        Dr
      ],
    },
    firstName: {
      type: string,
      occurs: required
    },
    middleName: string,
    lastName: {
      type: string,
      occurs: required
    },
    age: {
      type: int,
      valid_values: range::[
        0,
        130
      ]
    },
  },
}
